<UserControl 
			xmlns="https://github.com/avaloniaui"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            xmlns:vm="clr-namespace:PasswordManager.ViewModels"
            x:DataType="vm:CredentialListViewModel"
            x:Class="PasswordManager.Views.CredentialListView"
			xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			mc:Ignorable="d"
			xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			d:DesignWidth="600" d:DesignHeight="300"		 
>

	<StackPanel Margin="20" Spacing="10">
		<TextBlock Text="Stored Credentials" FontSize="20" FontWeight="Bold" />

		<TextBlock Text="(Here will be the list of saved credentials...)" FontStyle="Italic" />
	</StackPanel>

</UserControl>
