<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="PasswordManager.App"
			 xmlns:local="clr-namespace:PasswordManager"		 
             RequestedThemeVariant="Default"
>
	
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="avares://PasswordManager/Resources/IconResource.axaml" />
				<ResourceInclude Source="avares://PasswordManager/Resources/ConverterResource.axaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>


	<Application.Styles>
		<FluentTheme />
		<StyleInclude Source="avares://PasswordManager/Styles/FocusBlackTextBoxStyle.axaml" />
	</Application.Styles>
	
</Application>